{
  "name": "pkgif",
  "version": "0.5.0",
  "description": "pkgif",
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.dev.js --progress --inline --hot",
    "dev:server": "npm run lint && egg-bin dev",
    "build": "webpack --config webpack/webpack.prod.js --profile --progress --bail --display-error-details",
    "startup:test": "egg-scripts start --daemon --title=egg-server-pkgif --env=test --port=9100",
    "startup:prod": "egg-scripts start --daemon --title=egg-server-pkgif --env=prod --port=9100",
    "stop": "egg-scripts stop --title=egg-server-pkgif",
    "lint": "eslint --format node_modules/eslint-friendly-formatter ./app/",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "dependencies": {
    "egg": "^2.2.1",
    "egg-helper": "^1.1.2",
    "egg-mongoose": "^3.1.0",
    "egg-scripts": "^2.5.0",
    "egg-validate": "^1.0.0",
    "fs-extra": "^7.0.0",
    "jsonwebtoken": "^8.2.1",
    "nodemailer": "^4.6.8",
    "qiniu": "^7.1.5"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "autoprefixer": "^9.0.1",
    "axios": "^0.16.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-vue-app": "^2.0.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.7",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "fabric": "^2.3.6",
    "file-loader": "^1.1.11",
    "gif.js.optimized": "^1.0.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^3.2.0",
    "iview": "^3.0.0",
    "iview-loader": "^1.2.1",
    "jquery": "^3.3.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "mini-css-extract-plugin": "^0.4.0",
    "plupload": "^2.3.6",
    "postcss-loader": "^2.1.5",
    "qiniu-js": "^2.2.2",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.1.0",
    "url-loader": "^1.0.1",
    "vue": "^2.5.17",
    "vue-loader": "^15.2.0",
    "vue-router": "^3.0.1",
    "vue-slider-component": "^2.7.6",
    "vue-template-compiler": "^2.5.17",
    "vuedraggable": "^2.16.0",
    "vuex": "2.3.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.0",
    "webstorm-disable-index": "^1.2.0"
  }
}
